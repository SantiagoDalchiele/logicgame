				</pista>
				<pista>
					<id-dimension1>Marido</id-dimension1>
					<id-valor1>Maximiliano</id-valor1>
					<id-dimension2>Joya</id-dimension2>
					<id-valor2>plata</id-valor2>
					<afirma-niega>false</afirma-niega>
				</pista>
			</pistas>
		</pista-del-juego>
	</pistas-del-juego>
</juego>
